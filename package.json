{
  "name": "carina",
  "version": "0.5.4",
  "description": "A NodeJS and Browser compatible client for Beam.pro's constellation socket.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build:ts": "rm -rf lib && tsc",
    "build:webpack": "rm -rf dist && webpack && webpack --config webpack.config.min.js",
    "build": "npm run build:ts && npm run build:webpack",
    "test": "npm run build:ts -- --module commonjs && mocha -r test/_setup.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WatchBeam/carina.git"
  },
  "keywords": [
    "Constellation",
    "Beam"
  ],
  "author": "James Birtles <jameshb@beam.pro>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WatchBeam/carina/issues"
  },
  "homepage": "https://github.com/WatchBeam/carina#readme",
  "devDependencies": {
    "awesome-typescript-loader": "^3.0.3",
    "chai": "^3.5.0",
    "chai-subset": "^1.3.0",
    "mocha": "^3.0.2",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "ts-node": "^2.1.0",
    "typescript": "^2.1.6",
    "webpack": "^2.2.1",
    "ws": "^1.1.1"
  },
  "dependencies": {
    "@types/node": "^6.0.45",
    "@types/pako": "^0.2.31",
    "pako": "^1.0.3"
  }
}
